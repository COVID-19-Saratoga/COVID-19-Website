{
  "name": "togalink",
  "version": "1.0.0",
  "description": "Linking the Saratoga community to overcome COVID-19",
  "main": "index.html",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "postcss-cli": "^7.1.0",
    "postcss-preset-env": "^6.7.0"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:esm && npm run copy",
    "build:js": "babel src/ -d dist",
    "build:js:watch": "babel src/ -d dist --watch",
    "build:esm": "snowpack --include \"dist/**/*.js\" --dest dist/web_modules",
    "copy": "cpx index.html ./dist",
    "dev": "npm run build && npm run build:js:watch & serve -s dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TogaLink/TogaLink-Website.git"
  },
  "keywords": [
    "COVID-19",
    "support",
    "volunteer",
    "to",
    "help",
    "seniors",
    "Saratoga",
    "community",
    "help",
    "Saratogan",
    "seniors",
    "get",
    "groceries",
    "Saratoga",
    "COVID-19",
    "response"
  ],
  "author": "TogaLink Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TogaLink/TogaLink-Website/issues"
  },
  "homepage": "https://github.com/TogaLink/TogaLink-Website#readme",
  "browserslist": [
    "defaults"
  ]
}
